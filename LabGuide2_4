#include<stdio.h>

int main (void) {
	
	double num1,num2;
	
	char ch1,ch2;
	
	double avg;
	
	printf("Enter two double numbers:");
	scanf("%lf %lf",&num1,&num2);
	
	printf("Enter two characters:");
	scanf(" %c %c",&ch1,&ch2);
	
	avg=(num1+num2)/2;
	
	printf("Average is:%f",avg);
	
	printf("Two characters are: %c %c",ch1,ch2);
	
	
	return 0;
}
